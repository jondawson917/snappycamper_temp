{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.facility.com/facilityNew.schema.json",
  "type": "object",
  "properties": {
    "parkCode": {
      "type": "string",
      "minLength": 4,
      "maxLength": 4
    },
    "cellPhoneReception": {
      "type": "string"
    },
    "toilets": {
      "type": "string"
    },
    "boat_access": {
      "type": "boolean"
    },

    "rv_access": {
      "type": "boolean"
    },
    "wheelchair_access": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": ["parkCode", "cellPhoneReception", "toilets", "boat_access", "rv_access", "wheelchair_access"]
}